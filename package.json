        "@astrojs/tailwind": "^3.1.3",
        "@astrojs/ts-plugin": "^1.0.10",
        "@tailwindcss/forms": "^0.5.3",
        "@tailwindcss/typography": "^0.5.9",
        "@types/react": "^18.0.21",
        "@types/react-dom": "^18.0.6",
        "astro": "^2.5.0",
        "astro-auto-import": "^0.3.0",
        "astro-icon": "^0.8.1",
        "marked": "^5.0.2",
        "react": "^18.0.0",
        "react-dom": "^18.0.0",
